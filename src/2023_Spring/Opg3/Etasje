// 3i
public class Etasje {
  private int etasje;
  private Rom[] rom;

  public Etasje(int etasje, Rom[] rom) {
    this.etasje = etasje;
    this.rom = rom;
  }

// 3j
  @Override
  public String toString() {
    String tekst = "Etasje " + etasje + "\n";
    for(int i = 0; i < rom.length; i++) {
      tekst += rom[i].toString();
    }
    return tekst;
  }
}